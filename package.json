{
  "name": "myaias",
  "version": "2.0.1",
  "description": "A Vue 3 AI personal assistant component",
  "main": "./dist/myaias.umd.cjs",
  "license": "MIT",
  "module": "./dist/myaias.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
