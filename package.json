    "./dist/myaias.css": {
      "import": "./dist/myaias.css",
      "require": "./dist/myaias.css"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "vue",
    "vue3",
    "MyAIAS",
    "Personal AI Assistant"
  ],
  "author": "Yaya Sopegue SORO",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "scripts": {
