    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^12.3.0",
    "@vueuse/motion": "^2.2.6",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "vite-plugin-circular-dependency": "^0.5.0",
    "@types/jsdom": "^21.1.7",
